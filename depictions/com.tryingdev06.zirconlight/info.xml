<package>
	<id>com.tryingdev06.zirconlight</id>
	<name>Zircon Light</name>
	<version>0.1.3</version>
	<compatibility>
		<firmware>
			<miniOS>13.3.1</miniOS>
			<maxiOS>13.3.1</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>com.anemonetheming.anemone | com.spark.snowboard</dependencies>
	<descriptionlist>
		<description>This is a simple theme that features teal colors with a white background. It also includes recolored notification badges. The HTML color of the base teal is #00E2E6.</description>
	</descriptionlist>
	<screenshots></screenshots>
	<changelog>
		<change>Updated to include notification badges.</change>
	</changelog>
	<links></links>
</package>